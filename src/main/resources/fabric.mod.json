{
  "schemaVersion": 1,
  "id": "simpleelytrahud",
  "version": "${version}",
  "name": "SimpleElytraHud",
  "description": "An elytra HUD mod",
  "authors": ["Lukasabbe"],
  "contact": {
    "repo": "https://github.com/lukasabbe/TransportHud"
  },
  "license": "MIT",
  "icon": "assets/simpleelytrahud/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.lukasabbe.simpleelytrahud.SimpleElytraHudMod"
    ],
    "modmenu": [
      "me.lukasabbe.simpleelytrahud.config.ModMenu"
    ]
  },
  "mixins": [
    "simpleelytrahud.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": ">=1.21",
    "java": ">=21",
    "fabric-api-base": "*",
    "fabric-lifecycle-events-v1": "*",
    "fabric-resource-loader-v0": "*",
    "yet_another_config_lib_v3": ">=${yacl_version}"
  }
}
